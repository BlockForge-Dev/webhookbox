{
  "db_name": "PostgreSQL",
  "query": "\n        SELECT\n          d.id as \"delivery_id!\",\n          e.url as \"endpoint_url!\",\n          d.target_url as \"delivery_target_url\",\n          e.secret as \"endpoint_secret!\",\n          ev.id as \"event_id!\",\n          ev.payload_json as \"event_payload!\"\n        FROM deliveries d\n        JOIN endpoints e ON e.id = d.endpoint_id\n        JOIN events ev ON ev.id = d.event_id\n        WHERE d.id = $1\n        ",
